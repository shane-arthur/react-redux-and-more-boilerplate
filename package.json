{
  "name": "react-redux-boilerplate",
  "version": "1.0.0",
  "main": "src/js/app.js",
  "scripts": {
    "start": "cross-env concurrently --kill-others \"npm run start-dev-server\" \"npm run start-webpack-dev-server\"",
    "dev": "concurrently --kill-others \"npm run start-webpack-dev-server\" \"npm run start-dev-server\"",
    "start-webpack-dev-server": "cross-env NODE_ENV=development node ./webpack/run-webpack-server.js",
    "start-dev-server": "cross-env NODE_ENV=development nodemon src/js/server",
    "deploy": "NODE_ENV=production webpack -p --config ./webpack/webpack.production.config.js",
    "webpack": "webpack",
    "build": "rimraf dist/ && npm run webpack",
    "lint": "eslint ./src --cache",
    "test": "jest --coverage"
  },
  "dependencies": {
    "babel-core": "6.x",
    "babel-register": "6.x",
    "babel-runtime": "6.x",
    "bluebird": "^3.3.5",
    "body-parser": "^1.17.1",
    "compression": "^1.6.2",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.3",
    "express": "^4.13.3",
    "express-session": "^1.15.2",
    "file-loader": "^0.8.5",
    "fs": "0.0.1-security",
    "history": "^1.17.0",
    "http-proxy": "^1.15.1",
    "isomorphic-fetch": "^2.2.1",
    "method-override": "^2.3.8",
    "mongodb": "^2.1.16",
    "mongoose": "^4.4.12",
    "morgan": "^1.8.1",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "radium": "^0.18.1",
    "react": "15.x",
    "react-dom": "15.x",
    "react-redux": "^4.4.1",
    "react-router": "^3.0.0",
    "redux": "^3.4.0",
    "redux-thunk": "^2.2.0",
    "socket.io": "^2.0.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-isomorphic-tools": "^2.5.8"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-loader": "6.x",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "6.x",
    "babel-preset-es2015": "6.x",
    "babel-preset-react": "6.x",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "6.x",
    "concurrently": "^2.2.0",
    "copy-webpack-plugin": "latest",
    "cross-env": "^2.0.1",
    "ejs": "^2.5.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "jest": "^20.0.0",
    "nodemon": "^1.10.2",
    "react-transform-hmr": "^1.0.4",
    "standard": "^7.1.2",
    "supertest": "^3.0.0",
    "webpack-dev-server": "^1.16.2"
  },
  "keywords": [
    "React",
    "Redux",
    "ES6",
    "Webpack"
  ],
  "author": "Shane Arthur",
  "license": "MIT",
  "description": "React-redux and more boilerplate",
  "standard": {
    "parser": "babel-eslint"
  },
  "jest": {
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js"
    ],
    "globals": {
      "__DEVELOPMENT__": false,
      "__PICTURES__": null
    },
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/js/server/**.{js}",
      "!**/node_modules/**",
      "!src/js/***/**.{config.js}",
      "!src/js/server/data/startDb.js",
      "!src/js/server/index.js"
    ]
  },
  "repository": "https://github.com/shane-arthur/react-redux-and-more-boilerplate.git"
}
